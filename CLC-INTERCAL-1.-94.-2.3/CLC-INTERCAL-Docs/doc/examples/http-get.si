	DO ,3 SUB #9 <- #93
	DO ,3 SUB #10 <- #87
	DO ,3 SUB #11 <- #92
	DO ,3 SUB #12 <- #86
	DO ,3 SUB #13 <- #72
	DO ,3 SUB #14 <- #66
(666)	DO .1 <- #7
	DO READ OUT @1 + ;1 + ,2 + ,3
	PLEASE DO ,3 <- #4
	DO ,3 SUB #1 <- #91
